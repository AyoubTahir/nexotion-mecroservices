{
  "name": "@ayoubtahir/nexotion-shared",
  "version": "0.0.5",
  "license": "MIT",
  "description": "helper library for nexotion app",
  "main": "index.js",
  "types": "./types/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsx scripts/build.ts"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "author": "Tahir Ayoub",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AyoubTahir/nexotion-shared.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "express": "^4.21.1",
    "typescript": "^5.7.2",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  }
}
